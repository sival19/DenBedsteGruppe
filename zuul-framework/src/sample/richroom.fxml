<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="436.0" prefWidth="739.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RichroomController">
                  <children>
                     <ImageView fitHeight="452.0" fitWidth="740.0" layoutY="-1.0">
                        <image>
                           <Image url="@../images/richroom.PNG" />
                        </image>
                     </ImageView>
                     <ImageView fitHeight="225.0" fitWidth="159.0" layoutX="385.0" layoutY="196.0" onMouseClicked="#talkRichman">
