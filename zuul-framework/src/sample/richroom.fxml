<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="760.0" prefWidth="1150.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RichroomController">

    <!-- Inventory -->
    <Label fx:id="text" layoutX="8.0" layoutY="640.0" prefHeight="38.0" prefWidth="1135.0" />
    <ImageView fx:id="appleInv" fitHeight="86.0" fitWidth="98.0" layoutY="677.0" opacity="0.1">
        <Image url="@../images/apple.png" />
    </ImageView>
    <ImageView fx:id="bookInv" fitHeight="57.0" fitWidth="60.0" layoutX="118.0" layoutY="697.0" opacity="0.1">
        <Image url="@../images/book.png" />
    </ImageView>
    <ImageView fx:id="medicineInv" fitHeight="57.0" fitWidth="60.0" layoutX="519.0" layoutY="692.0" opacity="0.1">
        <Image url="@../images/medicine.png" />
    </ImageView>
    <ImageView fx:id="drawingInv" fitHeight="57.0" fitWidth="60.0" layoutX="419.0" layoutY="692.0" opacity="0.1" onMouseClicked="#interactDrawing" disable="true">
        <Image url="@../images/drawing.png" />
    </ImageView>
    <ImageView fx:id="moneyInv" fitHeight="57.0" fitWidth="60.0" layoutX="320.0" layoutY="692.0" onMouseClicked="#interactMoney" opacity="0.1">
        <Image url="@../images/money.png" />
    </ImageView>
    <ImageView fx:id="scarfInv" fitHeight="57.0" fitWidth="60.0" layoutX="219.0" layoutY="692.0" opacity="0.1">
        <Image url="@../images/scarf.png" />
    </ImageView>

    <!-- Layout inventory -->
    <Line endX="1150.0" endY="0.39996337890625" layoutY="678.0" startX="-1.220703143189894E-5" />
    <Line endX="-100.00000762939453" endY="-98.800048828125" layoutX="200.0" layoutY="779.0" startX="-100.00000762939453" startY="-13.79998779296875" />
    <Line endX="-100.00000762939453" endY="-98.800048828125" layoutX="300.0" layoutY="779.0" startX="-100.00000762939453" startY="-13.79998779296875" />
    <Line endX="-100.00000762939453" endY="-98.800048828125" layoutX="400.0" layoutY="779.0" startX="-100.00000762939453" startY="-13.79998779296875" />
    <Line endX="-100.00000762939453" endY="-98.800048828125" layoutX="500.0" layoutY="779.0" startX="-100.00000762939453" startY="-13.79998779296875" />
    <Line endX="-100.00000762939453" endY="-98.800048828125" layoutX="600.0" layoutY="779.0" startX="-100.00000762939453" startY="-13.79998779296875" />
    <Line endX="-100.00000762939453" endY="-98.800048828125" layoutX="700.0" layoutY="779.0" startX="-100.00000762939453" startY="-13.79998779296875" />
    <Line endX="-100.00000762939453" endY="-98.800048828125" layoutX="800.0" layoutY="779.0" startX="-100.00000762939453" startY="-13.79998779296875" />
    <Line endX="-100.00000762939453" endY="-98.800048828125" layoutX="900.0" layoutY="779.0" startX="-100.00000762939453" startY="-13.79998779296875" />
    <Line endX="-100.00000762939453" endY="-98.800048828125" layoutX="1000.0" layoutY="779.0" startX="-100.00000762939453" startY="-13.79998779296875" />
    <Line endX="-100.00000762939453" endY="-98.800048828125" layoutX="1100.0" layoutY="779.0" startX="-100.00000762939453" startY="-13.79998779296875" />
    <Line endX="-100.00000762939453" endY="-98.800048828125" layoutX="1200.0" layoutY="779.0" startX="-100.00000762939453" startY="-13.79998779296875" />

    <!-- Background -->
    <ImageView fitHeight="640.0" fitWidth="1150.0" layoutY="-1.0">
        <Image url="@../images/richroom.PNG" />
    </ImageView>

    <!-- People -->
    <ImageView fx:id="richman" fitHeight="315.0" fitWidth="231.0" layoutX="631.0" layoutY="264.0" onMouseClicked="#talkRichman">
        <Image url="@../images/richman.png" />
    </ImageView>

    <!-- Items -->
    <ImageView fx:id="money" fitHeight="87.0" fitWidth="104.0" layoutX="881.0" layoutY="536.0" onMouseClicked="#pickMoney">
        <Image url="@../images/money.png" />
    </ImageView>

    <!-- RoomItems -->
    <ImageView fx:id="apple" fitHeight="87.0" fitWidth="104.0" layoutX="497.0" layoutY="378.0" onMouseClicked="#pickApple">
        <Image url="@../images/apple.png" />
    </ImageView>

    <!-- Navigations -->
    <Button layoutX="469.0" layoutY="33.0" mnemonicParsing="false" onAction="#goLibrary" text="Bibliotek" />
    <Button layoutX="15.0" layoutY="396.0" mnemonicParsing="false" onAction="#goSchool" text="Skolen" />
    <Button layoutX="14.0" layoutY="131.0" mnemonicParsing="false" onAction="#goPharmacy" text="Apotek" />

    <!-- Not necessary or? -->
    <ImageView fx:id="backpack" fitHeight="104.0" fitWidth="93.0" layoutX="171.0" layoutY="65.0" onMouseClicked="#seeInventory">
        <Image url="@../images/backpack.png" />
    </ImageView>


    <!-- Buttons for inventory interraction -->
    <HBox fx:id="appleChoice" alignment="CENTER_LEFT" disable="true" layoutX="97.0" layoutY="640.0" prefHeight="40.0"
          prefWidth="1049.0" visible="false">
        <Text accessibleRole="TEXT_AREA" fontSmoothingType="LCD" strokeLineJoin="BEVEL" strokeWidth="0.0"
              text="Du holder et æble, hvad vil du gøre?  " textAlignment="RIGHT" wrappingWidth="502.4000000000001">
            <font>
                <Font size="15.0"/>
            </font>
        </Text>
        <Button fx:id="throwApple" mnemonicParsing="false" prefHeight="35.0" prefWidth="105.0" text="Give to" />
        <Button mnemonicParsing="false" prefHeight="35.0" prefWidth="97.0" text="Giv til pigen"/>
    </HBox>

    <HBox fx:id="bookChoice" alignment="CENTER_LEFT" disable="true" layoutX="97.0" layoutY="640.0" prefHeight="40.0"
          prefWidth="1049.0" visible="false">
        <Text accessibleRole="TEXT_AREA" fontSmoothingType="LCD" strokeLineJoin="BEVEL" strokeWidth="0.0"
              text="Du holder en bog, hvad vil du gøre?  " textAlignment="RIGHT" wrappingWidth="502.4000000000001">
            <font>
                <Font size="15.0"/>
            </font>
        </Text>
        <Button fx:id="throwBook" mnemonicParsing="false" prefHeight="35.0" prefWidth="105.0" text="Give to "/>
        <Button mnemonicParsing="false" prefHeight="35.0" prefWidth="97.0" text="Giv til pigen"/>
    </HBox>

    <HBox fx:id="scarfChoice" alignment="CENTER_LEFT" disable="true" layoutX="97.0" layoutY="640.0" prefHeight="40.0"
          prefWidth="1049.0" visible="false">
        <Text accessibleRole="TEXT_AREA" fontSmoothingType="LCD" strokeLineJoin="BEVEL" strokeWidth="0.0"
              text="Du holder et halstørklæde, hvad vil du gøre?  " textAlignment="RIGHT"
              wrappingWidth="502.4000000000001">
            <font>
                <Font size="15.0"/>
            </font>
        </Text>
        <Button fx:id="throwScarf" mnemonicParsing="false" prefHeight="35.0" prefWidth="105.0" text="Give to "/>
        <Button mnemonicParsing="false" prefHeight="35.0" prefWidth="97.0" text="Giv til pigen"/>
    </HBox>

    <HBox fx:id="moneyChoice" alignment="CENTER_LEFT" disable="true" layoutX="97.0" layoutY="640.0" prefHeight="40.0"
          prefWidth="1049.0" visible="false">
        <Text accessibleRole="TEXT_AREA" fontSmoothingType="LCD" strokeLineJoin="BEVEL" strokeWidth="0.0"
              text="Du holder nogle penge, hvad vil du gøre?  " textAlignment="RIGHT" wrappingWidth="502.4000000000001">
            <font>
                <Font size="15.0"/>
            </font>
        </Text>
        <Button fx:id="throwMoney" mnemonicParsing="false" prefHeight="35.0" prefWidth="105.0" text="Give to "/>
        <Button mnemonicParsing="false" prefHeight="35.0" prefWidth="97.0" text="Giv til pigen"/>
    </HBox>

    <!-- Den her er ændret onsdag aften AC-->
    <HBox fx:id="drawingChoice" alignment="CENTER_LEFT" disable="true" layoutX="97.0" layoutY="640.0" prefHeight="40.0"
          prefWidth="1049.0" visible="false">
        <Text accessibleRole="TEXT_AREA" fontSmoothingType="LCD" strokeLineJoin="BEVEL" strokeWidth="0.0"
              text="Du holder en tegning, hvad vil du gøre?  " textAlignment="RIGHT" wrappingWidth="502.4000000000001">
            <font>
                <Font size="15.0"/>
            </font>
        </Text>
        <Button mnemonicParsing="false" prefHeight="35.0" prefWidth="97.0" text="Giv til rigmanden" onMouseClicked="#giveDrawingRich"/>
    </HBox>

    <HBox fx:id="medicineChoice" alignment="CENTER_LEFT" disable="true" layoutX="97.0" layoutY="640.0" prefHeight="40.0"
          prefWidth="1049.0" visible="false">
        <Text accessibleRole="TEXT_AREA" fontSmoothingType="LCD" strokeLineJoin="BEVEL" strokeWidth="0.0"
              text="Du holder noget medicin, hvad vil du gøre?  " textAlignment="RIGHT"
              wrappingWidth="502.4000000000001">
            <font>
                <Font size="15.0"/>
            </font>
        </Text>
        <Button fx:id="throwMedicine" mnemonicParsing="false" prefHeight="35.0" prefWidth="105.0" text="Give to "/>
        <Button mnemonicParsing="false" prefHeight="35.0" prefWidth="97.0" text="Giv til pigen"/>
    </HBox>

</AnchorPane>
